.obj/release-shared/server.o: server.cpp server.h \
		infodata.h \
		serverbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/server.o server.cpp

.obj/release-shared/infodata.o: infodata.cpp infodata.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/infodata.o infodata.cpp

serverbase.h: serverbase.ui 
	$(UIC) serverbase.ui -o serverbase.h

serverbase.cpp: serverbase.h serverbase.ui 
	$(UIC) serverbase.ui -i serverbase.h -o serverbase.cpp

.obj/release-shared/serverbase.o: serverbase.cpp serverbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/serverbase.o serverbase.cpp

.obj/release-shared/moc_server.o: .moc/release-shared/moc_server.cpp  server.h infodata.h \
		serverbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_server.o .moc/release-shared/moc_server.cpp

.obj/release-shared/moc_serverbase.o: .moc/release-shared/moc_serverbase.cpp  serverbase.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_serverbase.o .moc/release-shared/moc_serverbase.cpp

.moc/release-shared/moc_server.cpp: $(MOC) server.h
	$(MOC) server.h -o .moc/release-shared/moc_server.cpp

.moc/release-shared/moc_serverbase.cpp: $(MOC) serverbase.h
