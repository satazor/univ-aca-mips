####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)../../../bin/" || $(MKDIR) "$(INSTALL_ROOT)../../../bin/"
	-$(INSTALL_FILE) "../../../bin/$(QMAKE_TARGET)" "$(INSTALL_ROOT)../../../bin/$(QMAKE_TARGET)"
	-strip "$(INSTALL_ROOT)../../../bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)../../../bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)../../../bin/"


install_phrasebooks: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)../../../phrasebooks/" || $(MKDIR) "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/danish.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/dutch.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/finnish.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/french.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/german.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/italian.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/norwegian.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/russian.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/spanish.qph" "$(INSTALL_ROOT)../../../phrasebooks/"
	-$(INSTALL_FILE) "../phrasebooks/swedish.qph" "$(INSTALL_ROOT)../../../phrasebooks/"


uninstall_phrasebooks: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/danish.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/dutch.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/finnish.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/french.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/german.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/italian.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/norwegian.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/russian.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/spanish.qph"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../phrasebooks/swedish.qph"
	-$(DEL_DIR) "$(INSTALL_ROOT)../../../phrasebooks/"


install: install_target install_phrasebooks  

uninstall: uninstall_target uninstall_phrasebooks  

